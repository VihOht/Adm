# Environment variables for Railway deployment

# Django settings
DEBUG=false
SECRET_KEY=your-super-secret-key-here

# Database (Railway will provide DATABASE_URL automatically if you add PostgreSQL)
# DATABASE_URL=postgresql://user:password@host:port/database

# Static files (optional, for custom CDN)
# STATIC_URL=/static/

# Allowed hosts (Railway will set RAILWAY_STATIC_URL automatically)
# ALLOWED_HOSTS=yourdomain.com,*.up.railway.app
